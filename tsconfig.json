{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": ["react-native-svg"],
    "jsx": "react-jsx", 
    "esModuleInterop": true,
    "resolveJsonModule": true, // Thêm nếu bạn sử dụng file JSON
    "noImplicitAny": true, // Kiểm tra biến không có kiểu dữ liệu
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "svg.d.ts"
, "context/**/*.ts",  // Bao gồm tất cả các file TypeScript trong thư mục context  
]
}
